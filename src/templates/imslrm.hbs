<?xml version="1.0"?>

<lom
  xmlns="http://ltsc.ieee.org/xsd/LOM" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://ltsc.ieee.org/xsd/LOM lomCustom.xsd">
  
  <general uniqueElementName="general">
    {{#each general.identifier}}
      <identifier>
        <catalog uniqueElementName="catalog">{{this.catalog}}</catalog>
        <entry uniqueElementName="entry">{{this.entry}}</entry>
      </identifier>
    {{/each}}
    
      <title uniqueElementName="title">
        {{#each general.title.title}}
          <string language="{{this.titleLang}}">{{this.title}}</string>
        {{/each}}
      </title>


    {{#each general.language}}
      <language>{{this.mainLang}}</language>
    {{/each}}


    {{#each general.description}}
      <description>
        <string language="{{this.descGeneralLang}}">{{this.Description}}</string>
      </description>
    {{/each}}  

    {{#each general.keyword}} 
      <keyword>
         <string language="{{this.keywordGeneralLang}}">{{this.keywordGeneral}}</string> 
      </keyword>
    {{/each}}   

    {{#each general.coverage}} 
      <coverage>
         <string language="{{this.coverageLang}}">{{this.coverage}}</string> 
      </coverage>
    {{/each}} 

    {{#each general.structure}} 
      <structure uniqueElementName="structure">
        <source uniqueElementName="source">LOM-ESv1.0</source>
        <value uniqueElementName="value">{{this.structuresGeneral_1}}</value> 
      </structure>
    {{/each}} 
     {{#each general.aggregationLevel}}
      <aggregationLevel uniqueElementName="aggregationLevel"> 
        <source uniqueElementName="source">LOM-ESv1.0</source>
        <value uniqueElementName="value">{{this.aggregationLevels_1}}</value>
      </aggregationLevel>
     {{/each}} 
  </general>      

{{#if lifeCycle}}
  <lifeCycle uniqueElementName="lifeCycle">
    {{#if lifeCycle.version}}
      <version uniqueElementName="version">
      {{#each lifeCycle.version}}
        <string language="{{this.lifeCycleLang}}">{{this.versionlifecycle1}}</string>
      {{/each}}
      </version>
    {{/if}}

    {{#each lifeCycle.status}}
      <status uniqueElementName="status">
        <source uniqueElementName="source">LOM-ESv1.0</source>
        <value uniqueElementName="value">{{this.statusLifeCycle_1_1}}</value>
      </status>
    {{/each}}  

  {{#each lifeCycle.contribute}}
      <contribute>

        <role uniqueElementName="role">
          <source uniqueElementName="source">LOM-ESv1.0</source>
          <value uniqueElementName="value">{{this.rolesLifeCycle}}</value>
        </role>
    
        <entity>  
          BEGIN:VCARD VERSION:3.0 FN:{{this.entity.nameContribLifeCycle}}
          EMAIL;TYPE=INTERNET:{{this.entity.emailContribLifeCycle}} ORG:{{this.entity.organContribLifeCycle}}
          END:VCARD
        </entity>
   
        <date uniqueElementName="date"> 
          <dateTime uniqueElementName="dateTime">{{this.dateContribLifeCycle}}</dateTime>
       
            <description>
            {{#each this.description }}
              <string language="{{this.ContribLifeCycleLang}}">{{this.DescriptionContribLifeCycle}}</string>
            {{/each}} 
            </description>
      
        </date>
 
      </contribute>
   {{/each}}     
  </lifeCycle>
{{/if}}   

  <metaMetadata uniqueElementName="metaMetadata">
      {{#each metaMetadata.indentifier}}
      <identifier>
            <catalog uniqueElementName="catalog">{{this.metametadataCatalog}}</catalog>
            <entry uniqueElementName="entry">{{this.metametadataEntry}}</entry>
      </identifier>
      {{/each}}
      
      {{#each metaMetadata.contribute}}
      <contribute>
      
        <role uniqueElementName="role">
          <source uniqueElementName="source">LOM-ESv1.0</source>
          <value uniqueElementName="value">{{this.rolesMetametadata}}</value>
        </role>
       
      
        <entity>  
          BEGIN:VCARD VERSION:3.0 FN:{{this.entity.nameContribMetametadata}}
          EMAIL;TYPE=INTERNET:{{this.entity.emailContribMetametadata}} ORG:{{this.entity.organContribMetametadata}}
          END:VCARD
        </entity>
           
        <date uniqueElementName="date"> 
          <dateTime uniqueElementName="dateTime">{{this.dateContribMetametadata}}</dateTime>
         
            <description>
            {{#each this.description }}
              <string language="{{this.ContribMetametadataLang}}">{{this.DescriptionContribMetametadata}}</string>
            {{/each}} 
            </description>
       
        </date>

      </contribute>
      {{/each}}    
  
      <metadataSchema>{{metaMetadata.metadataSchema}}</metadataSchema> 
     
      <language>{{metaMetadata.langMetametada}}</language>
    
  
  </metaMetadata>    

{{#if technical}}   
  <technical uniqueElementName="technical">
    {{#each technical.format}}
      <format>{{this.formatTechnicalValue}}</format>
    {{/each}}
    
    {{#if technical.size}}
      <size uniqueElementName="size">{{this.sizeTechnicalValue_1}}</size>
    {{/if}}
    
    {{#each technical.location}}
      <location>{{this.locationTechnicalValue}}</location> 
    {{/each}}
    
    {{#each technical.requeriments}} 
      <orComposite>
     
          <type uniqueElementName="type">        
            <source uniqueElementName="source">LOM-ESv1.0</source>
            <value uniqueElementName="value">{{this.typeTechnicalReq}}</value>
          </type>
        
          <name uniqueElementName="name">
            <source uniqueElementName="source">LOM-ESv1.0</source>
            <value uniqueElementName="value">{{this.nameTechnicalReq}}</value>
          </name>
     
          <minimumVersion uniqueElementName="minimumVersion">{{this.minVerTechnicalReq}}</minimumVersion>
    
          <maximumVersion uniqueElementName="maximumVersion">{{this.maxVerTechnicalReq}}</maximumVersion>
   

      </orComposite>
    {{/each}}

    {{#if technical.installationRemarks}}      
      <installationRemarks uniqueElementName="installationRemarks"> 
      {{#each technical.installationRemarks}}
          <string language="{{this.LangRemTech}}">{{this.installRemTechValue}}</string>
      {{/each}}
      </installationRemarks>
    {{/if}}
    
    {{#if technical.otherPlatformRequirements}}      
      <otherPlatformRequirements> 
      {{#each technical.otherPlatformRequirements}}
          <string language="{{this.LangOtherTech}}">{{this.requirementsRemTechValue}}</string>
      {{/each}}
      </otherPlatformRequirements>
    {{/if}}

       
      <duration uniqueElementName="duration">
        
          <duration uniqueElementName="duration">P{{technical.duration.durationYearsDurTech}}Y{{technical.duration.durationMonthsDurTech}}M{{technical.duration.durationDaysDurTech}}DT{{technical.duration.durationHoursDurTech}}H{{technical.duration.durationMinutesDurTech}}M{{technical.duration.durationSecondsDurTech}}S</duration>
       
        
          <description>
          {{#each technical.duration.description}}
              <string language="{{this.languageDescDurTech}}">{{this.DescriptionDurTech}}</string>
          {{/each}}
          </description>
        
      </duration>
   
  </technical>
{{/if}}

{{#if educational}}
  <educational>
 
    <interactivityType uniqueElementName="interactivityType">
      <source uniqueElementName="source">LOM-ESv1.0</source>
      <value uniqueElementName="value">{{educational.interactivityType}}</value>
    </interactivityType>

  
  {{#each educational.learningResourceType}}
    <learningResourceType>
      <source uniqueElementName="source">LOM-ESv1.0</source>
      <value uniqueElementName="value">{{this.resourceTypeEducationalValue}}</value>
    </learningResourceType>
  {{/each}}

  
    <interactivityLevel uniqueElementName="interactivityLevel">
      <source uniqueElementName="source">LOM-ESv1.0</source>
      <value uniqueElementName="value">{{educational.levelIntEducational}}</value>
    </interactivityLevel>


    <semanticDensity uniqueElementName="semanticDensity">
      <source uniqueElementName="source">LOM-ESv1.0</source>
      <value uniqueElementName="value">{{educational.levelDensEducational}}</value>
    </semanticDensity>


  {{#each educational.intendedEndUserRole}}
    <intendedEndUserRole>
      <source uniqueElementName="source">LOM-ESv1.0</source>
      <value uniqueElementName="value">{{this.endUserEducationalValue}}</value>
    </intendedEndUserRole>
  {{/each}}

  {{#each educational.context}}
    <context>
      <source uniqueElementName="source">LOM-ESv1.0</source>
      <value uniqueElementName="value">{{this.contextEducationalValue}}</value>
    </context>
  {{/each}}

  {{#each educational.typicalAgeRange}}
   <typicalAgeRange>
      <string language="{{this.languageRangeEducational}}">{{this.rangeAgeEducationalValue}}</string>
   </typicalAgeRange>
  {{/each}}

    <difficulty uniqueElementName="difficulty">
      <source uniqueElementName="source">LOM-ESv1.0</source>
      <value uniqueElementName="value">{{this.difficultyEducationalValue_1}}</value>
    </difficulty>
 
  
    <typicalLearningTime uniqueElementName="typicalLearningTime">
      
        <duration uniqueElementName="duration">P{{educational.typicalLearningTime.durationYearsEducational}}Y{{educational.typicalLearningTime.durationMonthsEducational}}M{{educational.typicalLearningTime.durationDaysEducational}}DT{{educational.typicalLearningTime.durationHoursEducational}}H{{educational.typicalLearningTime.durationMinutesEducational}}M{{educational.typicalLearningTime.durationSecondsEducational}}S</duration> 
      
     
        <description>
         {{#each educational.typicalLearningTime.description}} 
            <string language="{{this.langDurationEducational}}">{{this.DescriptionLearningEducational}}</string>
         {{/each}}
        </description>
      
    </typicalLearningTime>

  
  {{#each educational.description}}
    <description>
        <string language="{{this.langDescrEducational}}">{{this.descEducationUseValue}}</string>
    </description>
  {{/each}}
  
  {{#each educational.language}}
    <language>{{this.languageEducationalUseValue}}</language>
  {{/each}} 

  {{#each educational.cognitiveProcess}}
    <cognitiveProcess uniqueElementName="cognitiveProcess">
       <source uniqueElementName="source">LOM-ESv1.0</source>
       <value uniqueElementName="value">{{this.processcogEducationalValue}}</value>
    </cognitiveProcess>
  {{/each}}

  </educational>           
{{/if}}

  <rights uniqueElementName="rights">
  {{#if rights.cost}}
    <cost uniqueElementName="cost">
      <source uniqueElementName="source">LOM-ESv1.0</source>
      <value uniqueElementName="value">{{this.costRightsValue_1}}</value>
    </cost>
  {{/if}}

  {{#if rights.copyrightAndOtherRestrictions}}
    <copyrightAndOtherRestrictions uniqueElementName="copyrightAndOtherRestrictions">
      <source uniqueElementName="source">LOM-ESv1.0</source>
      <value uniqueElementName="value">{{this.copyrightRightsValue_1}}</value>
    </copyrightAndOtherRestrictions>
  {{/if}} 

  {{#if rights.description}}
    <description>
      <string language="{{this.descRightsLang}}">{{this.descRightsValue}}</string>
    </description>
  {{/if}} 
    <access uniqueElementName="access">
      <accessType uniqueElementName="accessType">
          <source uniqueElementName="source">LOM-ESv1.0</source>
          <value uniqueElementName="value">{{this.accessTypeRights_1}}</value>
      </accessType>
      
      <description>
          <string language="{{this.descRightsLang}}">{{this.descRightsValue}}</string>
      </description>
    </access>
  </rights>  
  
{{#each relation}}
  <relation>
  {{#if relation.kind}}
    <kind uniqueElementName="kind">
      <source uniqueElementName="source">LOM-ESv1.0</source>
      <value uniqueElementName="value">{{this.relationRelationsValue}}</value>
    </kind>
  {{/if}}

  {{#if relation.resource}}
    <resource uniqueElementName="resource">
    {{#if relation.resource.identifier}}
      <identifier>
          <catalog uniqueElementName="catalog">{{this.catalogRelations}}</catalog>
          <entry uniqueElementName="entry">{{this.entryRelations}}</entry>
      </identifier>
    {{/if}}

    {{#if relation.resource.description}}
      <description>
          <string language="{{this.relationRelationsLang}}">{{this.DescriptionRelationRelations}}</string>
      </description>
    {{/if}}
    </resource>
  {{/if}}
  </relation>
{{/each}}

{{#each annotation}}
  <annotation>
  {{#if annotation.entity}}
    <entity>
        BEGIN:VCARD VERSION:3.0 FN:{{this.nameAnnotations}}
        EMAIL;TYPE=INTERNET:{{this.emailAnnotations}} ORG:{{this.organAnnotations}} END:VCARD
    </entity>
  {{/if}}

  {{#if annotation.date}}
    <date uniqueElementName="date">    
    {{#if annotation.date.dateTime}}
        <dateTime uniqueElementName="dateTime">{{this.dataTime}}</dateTime>
    {{/if}}
        
     {{#if annotation.date.description}}
        <description>
          <string language="{{this.langDateAnnotations}}">{{this.DescriptionDateAnnotations}}</string>
        </description>  
     {{/if}}
    </date> 
  {{/if}}
  
  {{#if annotation.description}}
    <description>
        <string language="{{this.LangAnnotations}}">{{this.DescriptionAnnotations}}</string>
    </description>  
  {{/if}}
  </annotation>
{{/each}}

{{#each classification}}
  <classification>
  {{#if classification.purpose}}
    <purpose uniqueElementName="purpose">  
        <source uniqueElementName="source">LOM-ESv1.0</source>
        <value uniqueElementName="value">{{this.typePurposeClassification}}</value>
    </purpose>
  {{/if}}

  {{#each classification.taxonPath}}  
    <taxonPath>
    {{#if classification.taxonPath.source}}
        <source uniqueElementName="source">
            <string>{{this.sourceNameClassification}}</string>
        </source>  
    {{/if}}
    {{#each classification.taxonPath.taxon}}
        <taxon>
            <id>{{this.idTaxonClassification}}</id>
            <entry>
                <string language="{{this.langClassificationTaxon_1}}">{{this.nameTaxonClassification_1}}</string>
            </entry>
        </taxon>
    {{/each}}
    </taxonPath>
  {{/each}} 
  
  {{#if classification.description}}
    <description>  
       <string language="{{this.tituloLangTaxonClassification}}">{{this.DescriptionTaxonClassification}}</string>
    </description>
  {{/if}}

  {{#if classification.keyword}}
    <keyword>  
       <string language="{{this.titleLangKeywordTaxonClassification}}">{{this.KeywordTaxonClassification}}</string>
    </keyword>
  {{/if}}
  </classification>
{{/each}}  

</lom>
      
    
    
